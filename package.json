{
  "name": "@globocom/clappr-detach-plugin",
  "version": "0.5.0-rc.1",
  "description": "A Clappr plugin to detach the video",
  "main": "./dist/clappr-detach-plugin.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "build:analyze": "NODE_ENV=production webpack --mode production --json > stats.json; webpack-bundle-analyzer stats.json dist --port 1234 --default-sizes parsed",
    "watch": "node_modules/.bin/webpack --watch",
    "lint": "eslint --fix --ext .js ./src",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:globocom/clappr-detach-plugin.git"
  },
  "keywords": [
    "plugin",
    "detach",
    "Player",
    "Clappr"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.0",  
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",  
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.0.1",
    "css-loader": "^6.7.3",
    "html-loader": "^4.2.0",
    "interact.js": "1.2.6",
    "lodash.assign": "^4.2.0",
    "webpack-dev-server": "^4.11.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
